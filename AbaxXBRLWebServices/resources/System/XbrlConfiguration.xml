  <!-- 
  The Preload Section allows users to load taxonomies into memo on initialization of Processing Engine 
  Once the taxonomy is loaded then each document that references that docuemnt or namespace will utilize the 
  preloaded taxonomy rather than relaod the taxonomy.
  -->

  <Preload>
    <LoadSchema namespace="http://www.xbrl.org/2003/role" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://www.w3.org/2001/XMLSchema" defaultIgnoreDuringFind="true" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://www.w3.org/XML/1998/namespace" defaultIgnoreDuringFind="true" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://www.w3.org/1999/xlink" forceGrammarLoad="true">
      <AddReference namespace="http://www.w3.org/2001/XMLSchema" />
    </LoadSchema>
    <LoadSchema namespace="http://www.xbrl.org/2003/XLink" forceGrammarLoad="true">
      <AddReference namespace="http://www.w3.org/2001/XMLSchema" />
    </LoadSchema>
    <LoadSchema namespace="http://www.xbrl.org/2003/linkbase" forceGrammarLoad="true">
      <AddReference namespace="http://www.xbrl.org/2003/role" />
      <AddReference namespace="http://www.w3.org/2001/XMLSchema" />
    </LoadSchema>
    <LoadSchema namespace="http://www.xbrl.org/2003/instance" forceGrammarLoad="true">
      <AddReference namespace="http://www.xbrl.org/2003/role" />
      <AddReference namespace="http://www.w3.org/2001/XMLSchema" />
    </LoadSchema>
    <LoadSchema namespace="http://www.xbrl.org/2004/ref" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://www.xbrl.org/2006/ref" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://www.xbrl.org/2004/linkbase/formula" />
    <!--    <LoadSchema namespace="http://xbrl.org/2005/xbrldta" forceGrammarLoad="true"/> -->
    <LoadSchema namespace="http://xbrl.org/2005/xbrldt" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2006/xbrldi" forceGrammarLoad="true"/>

    <!-- XBRL Formula 2008 Preloads; these must remain ordered -->
    <LoadSchema namespace="http://xbrl.org/2008/generic" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/label" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/reference" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/variable" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/validation" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/assertion/consistency" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/assertion/value" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/assertion/existence" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/formula" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/boolean" forceGrammarLoad="true" />
    <LoadSchema namespace="http://xbrl.org/2008/filter/concept" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/dimension" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/entity" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/general" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/match" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/period" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/relative" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/segment-scenario" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/tuple" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/unit" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2008/filter/value" forceGrammarLoad="true"/>

    <!-- XBRL Org Table Linkbase 1.0 -->
    <LoadSchema namespace="http://xbrl.org/2014/table" forceGrammarLoad="true"/>
    <LoadSchema namespace="http://xbrl.org/2014/table/model" forceGrammarLoad="true"/>

    <LoadSchema namespace="http://xbrl.org/PWD/2013-05-17/table" forceGrammarLoad="true"/>
    <!--
    Define each taxonomy entry point that you wish to have preloaded in the following manner
    
    <LoadSchema namespace="enter namespace" forceGrammarLoad="true"/>	
    -->


  </Preload>

